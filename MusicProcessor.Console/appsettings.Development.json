{
  "Paths": {
    "MusicFolderPath": "X:\\Storage\\Music Storage",
    "SpotDLMetadata": "X:\\Storage\\Music\\# SpotDL",
    "MetadataFile": "X:\\Storage\\Music\\# SpotDL\\All.spotdl",
    "ExportedMetadata": "X:\\Storage\\Music\\Playlist.json"
  },
  "ConnectionStrings": {
    "SQLite": "X:\\Storage\\Music\\# Persistence\\music.sqlite"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "X:\\Storage\\Music\\# Persistence\\log.log",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithCorrelationId"
    ]
  }
}